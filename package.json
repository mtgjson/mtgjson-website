{
  "name": "mtgjson-website",
  "version": "2.1.0",
  "author": "MTGJSON Developers",
  "repository": "https://github.com/mtgjson/mtgjson-website",
  "license": "MIT",
  "engines": {
    "node": "^12.8.0",
    "yarn": "^1.17.3"
  },
  "scripts": {
    "dev": "vuepress dev docs",
    "build": "vuepress build docs",
    "lint": "yarn lint:js",
    "lint:js": "eslint --fix --ext .js,.vue docs/.vuepress",
    "purge": "rm -rf node_modules && yarn cache clean",
    "test": "jest && yarn build && yarn delete:build",
    "test:watch": "jest --watch",
    "test:dev": "concurrently \"yarn dev\" \"yarn test:watch\"",
    "delete:build": "rm -rf docs/.vuepress/dist",
    "heroku-postbuild": "yarn build",
    "schema-builder": "node ./utils/schema-builder.js"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "concurrently": "^4.1.0",
    "eslint": "^6.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "vue-eslint-parser": "^6.0.4",
    "vuepress": "^1.0.2",
    "vuepress-plugin-container": "^2.0.1",
    "vuepress-plugin-feed": "^0.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  }
}
