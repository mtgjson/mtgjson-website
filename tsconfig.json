{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "target": "es2015",
    "lib": [
      "es5"
    ],
    "paths": {
      "*": ["node_modules/*", "src/types/*"]
    },
    "sourceMap": false,
    "outDir": "docs/.vuepress/modules"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
