  "properties": {
    "artist": {
      "$id": "#/properties/artist",
      "type": "string",
      "title": "The Artist Schema",
      "default": "",
      "examples": [
        "Ryan Yee"
      ],
      "pattern": "^(.*)$"
    },
    "borderColor": {
      "$id": "#/properties/borderColor",
      "type": "string",
      "title": "The Bordercolor Schema",
      "default": "",
      "examples": [
        "black"
      ],
      "pattern": "^(.*)$"
    },
    "colorIdentity": {
      "$id": "#/properties/colorIdentity",
      "type": "array",
      "title": "The Coloridentity Schema",
      "items": {
        "$id": "#/properties/colorIdentity/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "W"
        ],
        "pattern": "^(.*)$"
      }
    },
    "colors": {
      "$id": "#/properties/colors",
      "type": "array",
      "title": "The Colors Schema",
      "items": {
        "$id": "#/properties/colors/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "W"
        ],
        "pattern": "^(.*)$"
      }
    },
    "convertedManaCost": {
      "$id": "#/properties/convertedManaCost",
      "type": "integer",
      "title": "The Convertedmanacost Schema",
      "default": 0,
      "examples": [
