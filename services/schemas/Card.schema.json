          "default": "",
          "examples": [
            "Legal"
          ],
          "pattern": "^(.*)$"
        },
        "vintage": {
          "$id": "#/properties/legalities/properties/vintage",
          "type": "string",
          "title": "The Vintage Schema",
          "default": "",
